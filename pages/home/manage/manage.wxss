page {
  background-color: #d6dcea;
}

.data-info {
  padding: 20rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
  margin: 20rpx 20rpx 0rpx 20rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  border-bottom: 1rpx #f5f5f5 solid;
  height: 60rpx;
}

.data-info .build {
  width: 38rpx;
  height: 37rpx;
}

.data-info .community {
  color: #333;
  font-size: 30rpx;
  flex: 1;
  margin-left: 10rpx;
}

.data-info .status {
  color: #fff;
  font-size: 24rpx;
  background-color: #70ad47;
  width: 104rpx;
  height: 41rpx;
  margin-right: 40rpx;
  border-radius: 5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.data-info .type {
  color: #333;
  font-size: 30rpx;
  margin-right: 10rpx;
}

.data-expand {
  padding: 0rpx 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  margin: 0rpx 20rpx;
  justify-content: center;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.data-expand image {
  width: 61rpx;
  height: 61rpx;
}

.action_box {
  height: 145rpx;
  justify-content: space-around;
  align-items: center;
  display: flex;
  width: 300rpx;
  border-top: 1rpx #f5f5f5 solid;
}

.active {
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.native {
  border-bottom-left-radius: 0rpx;
  border-bottom-right-radius: 0rpx;
}

.arrow {
  width: 12rpx;
  height: 8rpx;
  margin-right: 10rpx;
}

.infos {
  padding: 0rpx 20rpx;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin: 0rpx 20rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.infos .title {
  color: #666;
  font-size: 28rpx;
  display: flex;
}

.infos .title text {
  color: #999;
  font-size: 28rpx;
  flex: 1;
  text-align: right
}

.add {
  margin: 30rpx;
  display: flex;
  justify-content: center;
  height: 92rpx;
	background-color: #2bbcff;
	border-radius: 10rpx;
  color: #fff;
  font-size: 30rpx;
  align-items: center;
}
